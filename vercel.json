{
    "builds": [{
        "src": "GameGeeks/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],

    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/sttic/$1"
        },
        {    
        "src": "/(.*)",
        "dest": "GameGeeks/wsgi.py"
        }
    ]
}