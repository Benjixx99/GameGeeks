{
    "builds": [
        {
          "src": "GameGeeks/wsgi.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "GameGeeks/wsgi.py"
        }
      ],

      "outputDirectory": "staticfiles"
}